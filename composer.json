{
    "name": "kevinsr09/rumi-framework",
    "description": "simple framework php laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rumi\\": "src/"
        },
        "files": [
            "src/Helpers/Http.php",
            "src/Helpers/App.php",
            "src/Helpers/Strings.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rumi\\Tests\\": "tests/"
        },
        "files": [
            "src/Helpers/Http.php",
            "src/Helpers/App.php",
            "src/Helpers/Strings.php"
        ]
    },
    "authors": [
        {
            "name": "kevinsr09",
            "email": "kevinstivenrugeles@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "phpunit --color=always --testdox -v tests/"
    }
}
